{
  "name": "@plake492/form-validation",
  "version": "1.1.7",
  "main": "./dist/index.d.ts",
  "module": "./dist/index.d.ts",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --open --mode development --hot",
    "build": "npm run clean && yarn build:tsc && yarn copy-project-files && yarn copy-sass && yarn fix-package.json && yarn sass && yarn postcss-prod",
    "clean": "rm -rf dist",
    "build:tsc": "tsc",
    "copy-project-files": "cp package.json README.md ./dist ",
    "fix-package.json": "sed -i -- 's/index.d.ts/index.d.ts/g' ./dist/package.json",
    "sass": "sass --style=compressed src/assets/scss/index.scss:dist/assets/css/index.css",
    "postcss-prod": "node postcssConfig.js",
    "copy-sass": " cp -R src/assets ./dist/assets",
    "pub": "yarn build && yarn publish ./dist --access public"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "html-webpack-plugin": "^5.5.0",
    "postcss": "^8.4.21",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-nested": "^6.0.0",
    "postcss-sort-media-queries": "^4.3.0",
    "react-dom": "^18.2.0",
    "sass": "^1.59.3",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.12.0"
  },
  "dependencies": {
    "@types/react-transition-group": "^4.4.5",
    "date-fns": "^2.29.3",
    "react-transition-group": "^4.4.5"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
